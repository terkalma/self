         var CalendarRender = React.createClass({

  generateDataTable: function() {
    var firstDay = new Date(this.props.currentDay);
    var x = new Date(firstDay.setDate(1));

    alert(x);
    return (x); 
  },

  render: function() {
       
    return (<div className="today">
        Hi! The first day of the month is {JSON.stringify(this.generateDataTable())}
      </div>);
  }
})



